@model Movie

<h2>Confirm that you want to delete this record</h2>
<h3>@Model.Title</h3>

<!--This will provide a form with two buttons. One to delete and one to cancel which when clicked will go back to MovieList-->
<form method="post">

	<input type="hidden" asp-for="MovieId" />

	<button type="submit">Delete</button>
	<a class="btn btn-info" asp-controller="Home" asp-action="movieList">Cancel</a>
</form>